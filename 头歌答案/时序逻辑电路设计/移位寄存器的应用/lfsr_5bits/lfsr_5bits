    .port_info 0 /INPUT 1 "sys_clk";
    .port_info 1 /INPUT 1 "sys_rst_n";
    .port_info 2 /OUTPUT 5 "Q";
P_0x5613241a54e0 .param/l "N" 0 4 1, +C4<00000000000000000000000000000101>;
v0x561324191f50_0 .var "Q", 4 0;
v0x5613241bb4a0_0 .var "Q_next", 4 0;
v0x5613241bb580_0 .net "sys_clk", 0 0, v0x5613241bb880_0;  1 drivers
v0x5613241bb650_0 .net "sys_rst_n", 0 0, v0x5613241bb950_0;  1 drivers
E_0x5613241941a0 .event posedge, v0x5613241bb580_0;
E_0x561324193eb0 .event edge, v0x561324191f50_0;
    .scope S_0x5613241a5350;
T_0 ;
    %wait E_0x561324193eb0;
    %load/vec4 v0x561324191f50_0;
    %parti/s 4, 1, 2;
    %pad/u 5;
    %store/vec4 v0x5613241bb4a0_0, 0, 5;
    %load/vec4 v0x561324191f50_0;
    %parti/s 1, 0, 2;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5613241bb4a0_0, 4, 1;
    %load/vec4 v0x561324191f50_0;
    %parti/s 1, 3, 3;
    %load/vec4 v0x561324191f50_0;
    %parti/s 1, 0, 2;
    %xor;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5613241bb4a0_0, 4, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5613241a5350;
T_1 ;
    %wait E_0x5613241941a0;
    %load/vec4 v0x5613241bb650_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 1, 0, 5;
    %assign/vec4 v0x561324191f50_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5613241bb4a0_0;
    %assign/vec4 v0x561324191f50_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5613241a51c0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5613241bb880_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5613241bb950_0, 0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5613241bb950_0, 0;
    %end;
    .thread T_2;
    .scope S_0x5613241a51c0;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x5613241bb880_0;
    %inv;
    %store/vec4 v0x5613241bb880_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5613241a51c0;
T_4 ;
    %wait E_0x561324192350;
    %pushi/vec4 32, 0, 32;
T_4.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_4.1, 5;
    %jmp/1 T_4.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5613241941a0;
    %jmp T_4.0;
T_4.1 ;
    %pop/vec4 1;
    %vpi_call/w 3 34 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x5613241a51c0;
T_5 ;
    %vpi_call/w 3 40 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000000, "ns", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call/w 3 41 "$monitor", "At time %t: sys_rst_n = %b, Q_next = %b, Q = %b", $time, v0x5613241bb950_0, v0x5613241bb4a0_0, v0x5613241bb790_0 {0 0 0};
    %end;
    .thread T_5;
    .scope S_0x5613241a51c0;
T_6 ;
