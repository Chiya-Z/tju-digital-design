    .scope S_0x561356b9d680;
T_4 ;
    %delay 5, 0;
    %load/vec4 v0x561356bbaca0_0;
    %inv;
    %store/vec4 v0x561356bbaca0_0, 0, 1;
    %jmp T_4;
