    .scope S_0x560f10ad0b30;
T_5 ;
    %vpi_call/w 3 49 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call/w 3 50 "$dumpvars", 32'sb00000000000000000000000000000001, v0x560f10abe840_0 {0 0 0};
    %vpi_call/w 3 51 "$dumpvars", 32'sb00000000000000000000000000000001, v0x560f10abec00_0 {0 0 0};
