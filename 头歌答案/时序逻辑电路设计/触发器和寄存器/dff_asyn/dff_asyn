:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x560f10ac0a30 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x560f10ad0b30 .scope module, "tb_dff_asyn" "tb_dff_asyn" 3 5;
 .timescale -9 -9;
v0x560f10ae8140_0 .var "D", 0 0;
v0x560f10ae8200_0 .net "Q", 0 0, v0x560f10abe480_0;  1 drivers
v0x560f10ae82a0_0 .var "sys_clk", 0 0;
v0x560f10ae8370_0 .var "sys_rst_n", 0 0;
E_0x560f10ac0fc0 .event posedge, v0x560f10abe840_0;
S_0x560f10ad0cc0 .scope module, "U0" "dff_asyn" 3 11, 4 1 0, S_0x560f10ad0b30;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "sys_clk";
    .port_info 1 /INPUT 1 "sys_rst_n";
    .port_info 2 /INPUT 1 "D";
    .port_info 3 /OUTPUT 1 "Q";
v0x560f10abe110_0 .net "D", 0 0, v0x560f10ae8140_0;  1 drivers
v0x560f10abe480_0 .var "Q", 0 0;
v0x560f10abe840_0 .net "sys_clk", 0 0, v0x560f10ae82a0_0;  1 drivers
v0x560f10abec00_0 .net "sys_rst_n", 0 0, v0x560f10ae8370_0;  1 drivers
E_0x560f10ac13c0/0 .event negedge, v0x560f10abec00_0;
E_0x560f10ac13c0/1 .event posedge, v0x560f10abe840_0;
E_0x560f10ac13c0 .event/or E_0x560f10ac13c0/0, E_0x560f10ac13c0/1;
    .scope S_0x560f10ad0cc0;
T_0 ;
    %wait E_0x560f10ac13c0;
    %load/vec4 v0x560f10abec00_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x560f10abe480_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x560f10abe110_0;
    %assign/vec4 v0x560f10abe480_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x560f10ad0b30;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x560f10ae82a0_0;
    %inv;
    %store/vec4 v0x560f10ae82a0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x560f10ad0b30;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x560f10ae82a0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x560f10ae8370_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x560f10ae8140_0, 0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x560f10ae8370_0, 0;
    %end;
    .thread T_2;
    .scope S_0x560f10ad0b30;
T_3 ;
    %pushi/vec4 3, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x560f10ac0fc0;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x560f10ae8140_0, 0;
    %pushi/vec4 2, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x560f10ac0fc0;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x560f10ae8140_0, 0;
    %pushi/vec4 3, 0, 32;
T_3.4 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.5, 5;
    %jmp/1 T_3.5, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x560f10ac0fc0;
    %jmp T_3.4;
T_3.5 ;
    %pop/vec4 1;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x560f10ae8140_0, 0;
    %pushi/vec4 2, 0, 32;
T_3.6 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.7, 5;
    %jmp/1 T_3.7, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x560f10ac0fc0;
    %jmp T_3.6;
T_3.7 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x560f10ae8370_0, 0;
    %pushi/vec4 2, 0, 32;
T_3.8 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.9, 5;
    %jmp/1 T_3.9, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x560f10ac0fc0;
    %jmp T_3.8;
T_3.9 ;
    %pop/vec4 1;
    %vpi_call/w 3 36 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x560f10ad0b30;
T_4 ;
    %vpi_call/w 3 42 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000000, "ns", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call/w 3 43 "$monitor", "At time %t: sys_rst_n = %b, D = %b, Q = %b", $time, v0x560f10ae8370_0, v0x560f10ae8140_0, v0x560f10ae8200_0 {0 0 0};
    %end;
