:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x55d21216ba00 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55d21217d540 .scope module, "tb_pwm" "tb_pwm" 3 5;
 .timescale -9 -9;
v0x55d212193660_0 .net "o_pwm", 0 0, v0x55d21216a1a0_0;  1 drivers
v0x55d212193730_0 .var "sys_clk", 0 0;
v0x55d212193800_0 .var "sys_rst_n", 0 0;
E_0x55d21216bf40 .event posedge, v0x55d212193520_0;
S_0x55d21217d6d0 .scope module, "U0" "pwm" 3 10, 4 1 0, S_0x55d21217d540;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "sys_clk";
    .port_info 1 /INPUT 1 "sys_rst_n";
    .port_info 2 /OUTPUT 1 "o_pwm";
P_0x55d2121696f0 .param/l "CNT_MAX" 1 4 8, +C4<00000000000000000000000000001010>;
P_0x55d212169730 .param/l "HTIME" 0 4 1, +C4<00000000000000000000000000000011>;
P_0x55d212169770 .param/l "N" 0 4 1, +C4<00000000000000000000000000000100>;
P_0x55d2121697b0 .param/l "PERIOD" 0 4 1, +C4<00000000000000000000000000001010>;
v0x55d21216a1a0_0 .var "o_pwm", 0 0;
v0x55d212168960_0 .var "r_cnt", 3 0;
v0x55d212193450_0 .net "sys_clk", 0 0, v0x55d212193730_0;  1 drivers
v0x55d212193520_0 .net "sys_rst_n", 0 0, v0x55d212193800_0;  1 drivers
E_0x55d21216c0d0 .event posedge, v0x55d212193450_0;
    .scope S_0x55d21217d6d0;
T_0 ;
    %wait E_0x55d21216c0d0;
    %load/vec4 v0x55d212193520_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55d212168960_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55d212168960_0;
    %pad/u 32;
    %cmpi/e 9, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55d212168960_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x55d212168960_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55d212168960_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55d21217d6d0;
T_1 ;
    %wait E_0x55d21216c0d0;
    %load/vec4 v0x55d212193520_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55d21216a1a0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55d212168960_0;
    %pad/u 32;
    %cmpi/u 2, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz  T_1.2, 5;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55d21216a1a0_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55d21216a1a0_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55d21217d540;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55d212193730_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55d212193800_0, 0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55d212193800_0, 0;
    %end;
    .thread T_2;
    .scope S_0x55d21217d540;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x55d212193730_0;
    %inv;
    %store/vec4 v0x55d212193730_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55d21217d540;
T_4 ;
    %wait E_0x55d21216bf40;
    %pushi/vec4 20, 0, 32;
T_4.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_4.1, 5;
    %jmp/1 T_4.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55d21216c0d0;
    %jmp T_4.0;
T_4.1 ;
    %pop/vec4 1;
    %vpi_call/w 3 37 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x55d21217d540;
T_5 ;
    %vpi_call/w 3 44 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000000, "ns", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call/w 3 45 "$monitor", "At time %t: sys_rst_n = %b, o_pwm = %b", $time, v0x55d212193800_0, v0x55d212193660_0 {0 0 0};
    %end;
    .thread T_5;
    .scope S_0x55d21217d540;
T_6 ;
    %vpi_call/w 3 51 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call/w 3 52 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55d212193450_0 {0 0 0};
    %vpi_call/w 3 53 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55d212193520_0 {0 0 0};
    %vpi_call/w 3 54 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55d21216a1a0_0 {0 0 0};
    %end;
    .thread T_6;
# The file index is used to find the file name in the following table.
