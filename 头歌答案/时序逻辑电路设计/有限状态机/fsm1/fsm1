    .scope S_0x55a749243000;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55a74925deb0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55a74925dfb0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55a74925dd50_0, 0;
    %pushi/vec4 106, 0, 8;
    %assign/vec4 v0x55a74925e080_0, 0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55a74925dfb0_0, 0;
