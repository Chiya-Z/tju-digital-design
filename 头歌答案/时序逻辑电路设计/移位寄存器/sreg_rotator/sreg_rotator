    .scope S_0x557b200955a0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x557b200afaf0_0;
    %inv;
    %store/vec4 v0x557b200afaf0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x557b200955a0;
T_3 ;
    %wait E_0x557b200855b0;
    %wait E_0x557b200853f0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x557b200afa20_0, 0;
    %pushi/vec4 5, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x557b200853f0;
    %jmp T_3.0;
T_3.1 ;
