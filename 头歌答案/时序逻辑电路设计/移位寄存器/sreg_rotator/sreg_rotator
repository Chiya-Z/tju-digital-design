T_0 ;
    %wait E_0x557b200853f0;
    %load/vec4 v0x557b200af480_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x557b20082da0_0;
    %assign/vec4 v0x557b20083160_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x557b200af3c0_0;
    %cmpi/e 1, 0, 2;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x557b20083160_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x557b20083160_0;
    %parti/s 7, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x557b20083160_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x557b200af3c0_0;
    %cmpi/e 2, 0, 2;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x557b20083160_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x557b20083160_0;
    %parti/s 1, 7, 4;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x557b20083160_0, 0;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x557b200955a0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x557b200afaf0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x557b200afbe0_0, 0;
    %pushi/vec4 109, 0, 8;
    %assign/vec4 v0x557b200af7d0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x557b200afa20_0, 0;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x557b200af950_0, 0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x557b200afbe0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x557b200afa20_0, 0;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x557b200af950_0, 0;
    %end;
