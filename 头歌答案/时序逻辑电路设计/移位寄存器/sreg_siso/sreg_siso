    %parti/s 7, 0, 2;
    %load/vec4 v0x55b68151e020_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x55b6814f4d30_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55b6815075f0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55b68151e590_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55b68151e690_0, 0;
