T_0 ;
    %wait E_0x55b6814f7270;
    %load/vec4 v0x55b68151e270_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x55b6814f4d30_0, 0;
    %jmp T_0.1;
T_0.0 ;
