    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55b68151e690_0, 0;
    %end;
    .thread T_1;
    .scope S_0x55b6815075f0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55b68151e590_0;
    %inv;
    %store/vec4 v0x55b68151e590_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55b6815075f0;
T_3 ;
    %wait E_0x55b6814f6ec0;
    %pushi/vec4 24, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55b6814f7270;
    %vpi_func 3 37 "$random" 32 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %mod;
    %pad/u 1;
    %assign/vec4 v0x55b68151e400_0, 0;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %vpi_call/w 3 41 "$finish" {0 0 0};
    %end;
