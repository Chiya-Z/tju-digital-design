#! /usr/local/bin/vvp
:ivl_version "11.0 (stable)" "(v11_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x55815d2a2a50 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55815d2b44b0 .scope module, "tb_cnt_mod" "tb_cnt_mod" 3 5;
 .timescale -9 -9;
v0x55815d2c9440_0 .net "cnt", 3 0, v0x55815d2a1260_0;  1 drivers
v0x55815d2c9530_0 .var "sys_clk", 0 0;
v0x55815d2c9600_0 .var "sys_rst_n", 0 0;
S_0x55815d2b4640 .scope module, "U0" "cnt_mod" 3 10, 4 1 0, S_0x55815d2b44b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "sys_clk";
    .port_info 1 /INPUT 1 "sys_rst_n";
    .port_info 2 /OUTPUT 4 "cnt";
P_0x55815d29ef00 .param/l "CNT_MAX" 1 4 7, +C4<00000000000000000000000000001010>;
P_0x55815d29ef40 .param/l "N" 0 4 1, +C4<00000000000000000000000000000100>;
v0x55815d2a1260_0 .var "cnt", 3 0;
v0x55815d2c9230_0 .net "sys_clk", 0 0, v0x55815d2c9530_0;  1 drivers
v0x55815d2c92f0_0 .net "sys_rst_n", 0 0, v0x55815d2c9600_0;  1 drivers
E_0x55815d2a2f90 .event posedge, v0x55815d2c9230_0;
    .scope S_0x55815d2b4640;
T_0 ;
    %wait E_0x55815d2a2f90;
    %load/vec4 v0x55815d2c92f0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55815d2a1260_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55815d2a1260_0;
    %pad/u 32;
    %cmpi/e 9, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55815d2a1260_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x55815d2a1260_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55815d2a1260_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55815d2b44b0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55815d2c9530_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55815d2c9600_0, 0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55815d2c9600_0, 0;
    %end;
    .thread T_1;
    .scope S_0x55815d2b44b0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55815d2c9530_0;
    %inv;
    %store/vec4 v0x55815d2c9530_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55815d2b44b0;
T_3 ;
    %pushi/vec4 16, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x55815d2a2f90;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %vpi_call/w 3 31 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55815d2b44b0;
T_4 ;
    %vpi_call/w 3 37 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000000, "ns", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call/w 3 38 "$monitor", "At time %t: sys_rst_n = %b, cnt = %b", $time, v0x55815d2c9600_0, v0x55815d2c9440_0 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x55815d2b44b0;
T_5 ;
    %vpi_call/w 3 44 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call/w 3 45 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55815d2c9230_0 {0 0 0};
    %vpi_call/w 3 46 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55815d2c92f0_0 {0 0 0};
    %vpi_call/w 3 47 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55815d2a1260_0 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "tb_cnt_mod.v";
    "cnt_mod.v";
