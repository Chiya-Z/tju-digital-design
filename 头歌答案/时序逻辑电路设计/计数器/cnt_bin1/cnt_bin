    %assign/vec4 v0x5559a7970ad0_0, 0;
    %pushi/vec4 3, 0, 32;
T_3.4 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.5, 5;
    %jmp/1 T_3.5, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5559a794a2d0;
    %jmp T_3.4;
T_3.5 ;
    %pop/vec4 1;
    %vpi_call/w 3 34 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5559a795af10;
T_4 ;
