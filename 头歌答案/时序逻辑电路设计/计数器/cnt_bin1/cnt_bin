T_0 ;
    %wait E_0x5559a794a2d0;
    %load/vec4 v0x5559a79484b0_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5559a7947d90_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5559a7947d90_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x5559a7947d90_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5559a795af10;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5559a7970a30_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5559a7970ad0_0, 0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x5559a7970ad0_0, 0;
    %end;
    .thread T_1;
    .scope S_0x5559a795af10;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5559a7970a30_0;
    %inv;
    %store/vec4 v0x5559a7970a30_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5559a795af10;
T_3 ;
    %pushi/vec4 18, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5559a794a2d0;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5559a7970ad0_0, 0;
    %pushi/vec4 3, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x5559a794a2d0;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
