v0x559482358ab0_0 .var "sys_rst_n", 0 0;
v0x559482358bb0_0 .var "up", 0 0;
S_0x559482342660 .scope module, "U0" "cnt_ud" 3 11, 4 1 0, S_0x5594823424d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "sys_clk";
    .port_info 1 /INPUT 1 "sys_rst_n";
    .port_info 2 /INPUT 1 "up";
    .port_info 3 /OUTPUT 4 "cnt";
P_0x559482342840 .param/l "N" 0 4 1, +C4<00000000000000000000000000000100>;
v0x55948232f510_0 .var "cnt", 3 0;
v0x55948232f880_0 .net "sys_clk", 0 0, v0x5594823589e0_0;  1 drivers
v0x5594823586c0_0 .net "sys_rst_n", 0 0, v0x559482358ab0_0;  1 drivers
v0x559482358790_0 .net "up", 0 0, v0x559482358bb0_0;  1 drivers
E_0x559482331710 .event posedge, v0x55948232f880_0;
    .scope S_0x559482342660;
T_0 ;
    %wait E_0x559482331710;
    %load/vec4 v0x5594823586c0_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55948232f510_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x559482358790_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x55948232f510_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55948232f510_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x55948232f510_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x55948232f510_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5594823424d0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5594823589e0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x559482358ab0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x559482358bb0_0, 0;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x559482358ab0_0, 0;
    %end;
    .thread T_1;
    .scope S_0x5594823424d0;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5594823589e0_0;
    %inv;
    %store/vec4 v0x5594823589e0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5594823424d0;
T_3 ;
    %pushi/vec4 10, 0, 32;
T_3.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.1, 5;
    %jmp/1 T_3.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x559482331710;
    %jmp T_3.0;
T_3.1 ;
    %pop/vec4 1;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x559482358bb0_0, 0;
    %pushi/vec4 6, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x559482331710;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %vpi_call/w 3 35 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5594823424d0;
T_4 ;
    %vpi_call/w 3 41 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000000, "ns", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call/w 3 42 "$monitor", "At time %t: sys_rst_n = %b, up = %b, cnt = %b", $time, v0x559482358ab0_0, v0x559482358bb0_0, v0x559482358900_0 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x5594823424d0;
T_5 ;
    %vpi_call/w 3 48 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call/w 3 49 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55948232f880_0 {0 0 0};
    %vpi_call/w 3 50 "$dumpvars", 32'sb00000000000000000000000000000001, v0x5594823586c0_0 {0 0 0};
