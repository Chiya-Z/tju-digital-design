    .scope S_0x561b1cfa8220;
T_6 ;
    %vpi_call/w 3 36 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call/w 3 37 "$dumpvars", 32'sb00000000000000000000000000000001, v0x561b1cfbfc80_0 {0 0 0};
