    .scope S_0x561b1cfa8220;
T_5 ;
    %vpi_call/w 3 29 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000000, "ns", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call/w 3 30 "$monitor", "At time %t: d = %b, q = %b", $time, v0x561b1cfbff50_0, v0x561b1cfc0010_0 {0 0 0};
    %end;
