 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "d";
    .port_info 2 /OUTPUT 1 "q";
v0x561b1cfa8bc0_0 .net "clk", 0 0, v0x561b1cfbfeb0_0;  alias, 1 drivers
v0x561b1cf95180_0 .net "d", 0 0, v0x561b1cfbff50_0;  alias, 1 drivers
v0x561b1cf805d0_0 .var "q", 0 0;
E_0x561b1cf98290 .event posedge, v0x561b1cfa8bc0_0;
S_0x561b1cfbf090 .scope module, "DUT1" "dff" 4 10, 5 1 0, S_0x561b1cfa83b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "d";
    .port_info 2 /OUTPUT 1 "q";
v0x561b1cfbf2c0_0 .net "clk", 0 0, v0x561b1cfbfeb0_0;  alias, 1 drivers
v0x561b1cfbf380_0 .net "d", 0 0, v0x561b1cf805d0_0;  alias, 1 drivers
v0x561b1cfbf450_0 .var "q", 0 0;
S_0x561b1cfbf560 .scope module, "DUT2" "dff" 4 11, 5 1 0, S_0x561b1cfa83b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "d";
    .port_info 2 /OUTPUT 1 "q";
v0x561b1cfbf7c0_0 .net "clk", 0 0, v0x561b1cfbfeb0_0;  alias, 1 drivers
v0x561b1cfbf8b0_0 .net "d", 0 0, v0x561b1cfbf450_0;  alias, 1 drivers
v0x561b1cfbf970_0 .var "q", 0 0;
    .scope S_0x561b1cfa6480;
T_0 ;
    %wait E_0x561b1cf98290;
    %load/vec4 v0x561b1cf95180_0;
    %assign/vec4 v0x561b1cf805d0_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x561b1cfbf090;
T_1 ;
    %wait E_0x561b1cf98290;
    %load/vec4 v0x561b1cfbf380_0;
    %assign/vec4 v0x561b1cfbf450_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x561b1cfbf560;
T_2 ;
    %wait E_0x561b1cf98290;
    %load/vec4 v0x561b1cfbf8b0_0;
    %assign/vec4 v0x561b1cfbf970_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x561b1cfa8220;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561b1cfbfeb0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561b1cfbff50_0, 0;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561b1cfbff50_0, 0;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x561b1cfbff50_0, 0;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561b1cfbff50_0, 0;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561b1cfbff50_0, 0;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x561b1cfbff50_0, 0;
    %delay 15, 0;
    %vpi_call/w 3 22 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x561b1cfa8220;
T_4 ;
    %delay 5, 0;
    %load/vec4 v0x561b1cfbfeb0_0;
    %inv;
    %store/vec4 v0x561b1cfbfeb0_0, 0, 1;
    %jmp T_4;
