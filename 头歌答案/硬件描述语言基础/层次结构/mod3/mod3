 .timescale 0 0;
S_0x561b1cfa8220 .scope module, "tb_mod3" "tb_mod3" 3 3;
 .timescale -9 -9;
v0x561b1cfbfeb0_0 .var "clk", 0 0;
v0x561b1cfbff50_0 .var "d", 0 0;
