:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x55ee8912e230 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55ee89105da0 .scope module, "tb_vector100r" "tb_vector100r" 3 3;
 .timescale -9 -9;
v0x55ee89154ae0_0 .var "in", 99 0;
v0x55ee89154bd0_0 .net "out", 99 0, v0x55ee891549c0_0;  1 drivers
S_0x55ee89105f30 .scope module, "U0" "vector100r" 3 12, 4 1 0, S_0x55ee89105da0;
 .timescale 0 0;
    .port_info 0 /INPUT 100 "in";
    .port_info 1 /OUTPUT 100 "out";
v0x55ee891185d0_0 .var/2s "i", 31 0;
v0x55ee891548e0_0 .net "in", 99 0, v0x55ee89154ae0_0;  1 drivers
v0x55ee891549c0_0 .var "out", 99 0;
E_0x55ee8912e840 .event edge, v0x55ee891548e0_0;
    .scope S_0x55ee89105f30;
T_0 ;
    %wait E_0x55ee8912e840;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55ee891185d0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x55ee891185d0_0;
    %cmpi/s 100, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x55ee891548e0_0;
    %load/vec4 v0x55ee891185d0_0;
    %part/s 1;
    %pushi/vec4 99, 0, 32;
    %load/vec4 v0x55ee891185d0_0;
    %sub;
    %ix/vec4/s 4;
    %store/vec4 v0x55ee891549c0_0, 4, 1;
    %load/vec4 v0x55ee891185d0_0;
    %addi 1, 0, 32;
    %cast2;
    %store/vec4 v0x55ee891185d0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55ee89105da0;
T_1 ;
    %pushi/vec4 1024, 0, 100;
    %store/vec4 v0x55ee89154ae0_0, 0, 100;
    %delay 5, 0;
    %pushi/vec4 255, 0, 100;
    %store/vec4 v0x55ee89154ae0_0, 0, 100;
    %delay 5, 0;
    %pushi/vec4 99, 0, 100;
    %store/vec4 v0x55ee89154ae0_0, 0, 100;
    %delay 5, 0;
    %pushi/vec4 4567, 0, 100;
    %store/vec4 v0x55ee89154ae0_0, 0, 100;
    %delay 5, 0;
    %pushi/vec4 8899, 0, 100;
    %store/vec4 v0x55ee89154ae0_0, 0, 100;
    %delay 5, 0;
    %vpi_call/w 3 20 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55ee89105da0;
T_2 ;
    %vpi_call/w 3 25 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000000, "ns", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call/w 3 26 "$monitor", "At time %t: in = %d, out = %d", $time, v0x55ee89154ae0_0, v0x55ee89154bd0_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55ee89105da0;
T_3 ;
    %vpi_call/w 3 32 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call/w 3 33 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55ee891548e0_0 {0 0 0};
    %vpi_call/w 3 34 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55ee891549c0_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
