#! /usr/local/bin/vvp
:ivl_version "11.0 (stable)" "(v11_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x558138529fc0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55813853a6b0 .scope module, "tb_alwaysblock" "tb_alwaysblock" 3 3;
 .timescale -9 -9;
v0x558138550290_0 .var "a", 0 0;
v0x558138550350_0 .var "b", 0 0;
v0x558138550420_0 .net "out_alwaysblock", 0 0, v0x558138550050_0;  1 drivers
v0x558138550520_0 .net "out_assign", 0 0, L_0x5581385505f0;  1 drivers
S_0x55813853a840 .scope module, "U0" "alwaysblock" 3 12, 4 1 0, S_0x55813853a6b0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "out_assign";
    .port_info 3 /OUTPUT 1 "out_alwaysblock";
L_0x5581385505f0 .functor AND 1, v0x558138550290_0, v0x558138550350_0, C4<1>, C4<1>;
v0x55813852ae30_0 .net "a", 0 0, v0x558138550290_0;  1 drivers
v0x55813854ff90_0 .net "b", 0 0, v0x558138550350_0;  1 drivers
v0x558138550050_0 .var "out_alwaysblock", 0 0;
v0x558138550120_0 .net "out_assign", 0 0, L_0x5581385505f0;  alias, 1 drivers
E_0x55813852a4c0 .event edge, v0x55813852ae30_0, v0x55813854ff90_0;
    .scope S_0x55813853a840;
T_0 ;
    %wait E_0x55813852a4c0;
    %load/vec4 v0x55813852ae30_0;
    %load/vec4 v0x55813854ff90_0;
    %and;
    %store/vec4 v0x558138550050_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55813853a6b0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558138550290_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558138550350_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558138550290_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558138550350_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558138550290_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x558138550350_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558138550290_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x558138550350_0, 0, 1;
    %delay 5, 0;
    %vpi_call/w 3 23 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55813853a6b0;
T_2 ;
    %vpi_call/w 3 28 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000000, "ns", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call/w 3 29 "$monitor", "At time %t: a = %b, b = %b, out_assign = %b, out_alwaysblock = %b", $time, v0x558138550290_0, v0x558138550350_0, v0x558138550520_0, v0x558138550420_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55813853a6b0;
T_3 ;
    %vpi_call/w 3 35 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call/w 3 36 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55813852ae30_0 {0 0 0};
    %vpi_call/w 3 37 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55813854ff90_0 {0 0 0};
    %vpi_call/w 3 38 "$dumpvars", 32'sb00000000000000000000000000000001, v0x558138550120_0 {0 0 0};
    %vpi_call/w 3 39 "$dumpvars", 32'sb00000000000000000000000000000001, v0x558138550050_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 5;
