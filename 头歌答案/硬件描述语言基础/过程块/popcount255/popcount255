S_0x55c10c0df690 .scope begin, "$ivl_for_loop0" "$ivl_for_loop0" 4 12, 4 12 0, S_0x55c10c0eebf0;
 .timescale 0 0;
v0x55c10c0c75d0_0 .var/2s "i", 31 0;
    .scope S_0x55c10c0eebf0;
T_0 ;
    %wait E_0x55c10c0df140;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55c10c106e70_0, 0, 8;
    %fork t_1, S_0x55c10c0df690;
    %jmp t_0;
    .scope S_0x55c10c0df690;
t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c10c0c75d0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x55c10c0c75d0_0;
    %cmpi/s 255, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x55c10c106e70_0;
    %load/vec4 v0x55c10c106d70_0;
    %load/vec4 v0x55c10c0c75d0_0;
    %part/s 1;
    %pad/u 8;
    %add;
    %store/vec4 v0x55c10c106e70_0, 0, 8;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x55c10c0c75d0_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %cast2;
    %store/vec4 v0x55c10c0c75d0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x55c10c0eebf0;
t_0 %join;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55c10c0eea60;
T_1 ;
    %pushi/vec4 0, 0, 255;
    %store/vec4 v0x55c10c106fb0_0, 0, 255;
    %delay 20, 0;
    %pushi/vec4 1, 0, 255;
    %store/vec4 v0x55c10c106fb0_0, 0, 255;
    %delay 20, 0;
    %pushi/vec4 3, 0, 255;
    %store/vec4 v0x55c10c106fb0_0, 0, 255;
    %delay 20, 0;
    %pushi/vec4 7, 0, 255;
    %store/vec4 v0x55c10c106fb0_0, 0, 255;
    %delay 25, 0;
    %pushi/vec4 43690, 0, 255;
    %store/vec4 v0x55c10c106fb0_0, 0, 255;
    %delay 25, 0;
    %pushi/vec4 983040, 0, 255;
    %store/vec4 v0x55c10c106fb0_0, 0, 255;
    %delay 20, 0;
    %vpi_call/w 3 21 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55c10c0eea60;
T_2 ;
