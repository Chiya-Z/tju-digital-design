#! /usr/local/bin/vvp
:ivl_version "11.0 (stable)" "(v11_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x55f61cda20d0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55f61cdb31b0 .scope module, "tb_reduc" "tb_reduc" 3 3;
 .timescale -9 -9;
v0x55f61cdc6f20_0 .var "in", 7 0;
v0x55f61cdc6fe0_0 .net "parity", 0 0, L_0x55f61cdc70b0;  1 drivers
S_0x55f61cdb3340 .scope module, "U0" "reduc" 3 8, 4 1 0, S_0x55f61cdb31b0;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "in";
    .port_info 1 /OUTPUT 1 "parity";
v0x55f61cd8d5d0_0 .net "in", 7 0, v0x55f61cdc6f20_0;  1 drivers
v0x55f61cdc6e00_0 .net "parity", 0 0, L_0x55f61cdc70b0;  alias, 1 drivers
L_0x55f61cdc70b0 .reduce/xor v0x55f61cdc6f20_0;
    .scope S_0x55f61cdb31b0;
T_0 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55f61cdc6f20_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x55f61cdc6f20_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x55f61cdc6f20_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 21, 0, 8;
    %store/vec4 v0x55f61cdc6f20_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 167, 0, 8;
    %store/vec4 v0x55f61cdc6f20_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 38, 0, 8;
    %store/vec4 v0x55f61cdc6f20_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 184, 0, 8;
    %store/vec4 v0x55f61cdc6f20_0, 0, 8;
    %delay 5, 0;
    %pushi/vec4 255, 0, 8;
    %store/vec4 v0x55f61cdc6f20_0, 0, 8;
    %delay 5, 0;
    %vpi_call/w 3 19 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x55f61cdb31b0;
T_1 ;
    %vpi_call/w 3 24 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000000, "ns", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call/w 3 25 "$monitor", "At time %t: in = %x(%b),  parity = %b", $time, v0x55f61cdc6f20_0, v0x55f61cdc6f20_0, v0x55f61cdc6fe0_0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55f61cdb31b0;
T_2 ;
    %vpi_call/w 3 31 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call/w 3 32 "$dumpvars", 32'sb00000000000000000000000000000001, S_0x55f61cdb3340 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 5;
