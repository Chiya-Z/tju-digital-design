L_0x55eb1f8ec550 .shift/l 8, v0x55eb1f8ec070_0, v0x55eb1f8ec150_0;
L_0x55eb1f8ec6e0 .shift/r 8, v0x55eb1f8ec070_0, v0x55eb1f8ec150_0;
L_0x55eb1f8ec7d0 .shift/l 8, v0x55eb1f8ec070_0, v0x55eb1f8ec150_0;
L_0x55eb1f8ec950 .shift/rs 8, v0x55eb1f8ec070_0, v0x55eb1f8ec150_0;
    .scope S_0x55eb1f8d38c0;
T_0 ;
    %pushi/vec4 8, 0, 8;
    %store/vec4 v0x55eb1f8ec070_0, 0, 8;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x55eb1f8ec150_0, 0, 3;
