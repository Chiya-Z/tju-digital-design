#! /usr/local/bin/vvp
:ivl_version "11.0 (stable)" "(v11_0)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
:vpi_module "/usr/local/lib/ivl/v2009.vpi";
S_0x55cf2dd184b0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x55cf2dcf0da0 .scope module, "tb_wire1" "tb_wire1" 3 3;
 .timescale -9 -9;
v0x55cf2dd3dbe0_0 .var "in", 0 0;
v0x55cf2dd3dc80_0 .net "out", 0 0, L_0x55cf2dd3dd50;  1 drivers
S_0x55cf2dcf0f30 .scope module, "U0" "wire1" 3 8, 4 1 0, S_0x55cf2dcf0da0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "in";
    .port_info 1 /OUTPUT 1 "out";
L_0x55cf2dd3dd50 .functor BUFZ 1, v0x55cf2dd3dbe0_0, C4<0>, C4<0>, C4<0>;
v0x55cf2dd035d0_0 .net "in", 0 0, v0x55cf2dd3dbe0_0;  1 drivers
v0x55cf2dd3dac0_0 .net "out", 0 0, L_0x55cf2dd3dd50;  alias, 1 drivers
    .scope S_0x55cf2dcf0da0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cf2dd3dbe0_0, 0, 1;
    %delay 40, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cf2dd3dbe0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cf2dd3dbe0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cf2dd3dbe0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cf2dd3dbe0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cf2dd3dbe0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cf2dd3dbe0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cf2dd3dbe0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cf2dd3dbe0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55cf2dd3dbe0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55cf2dd3dbe0_0, 0, 1;
    %delay 5, 0;
    %vpi_call/w 3 22 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x55cf2dcf0da0;
T_1 ;
    %vpi_call/w 3 27 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000000, "ns", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call/w 3 28 "$monitor", "At time %t: in = %b, out = %b", $time, v0x55cf2dd3dbe0_0, v0x55cf2dd3dc80_0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55cf2dcf0da0;
T_2 ;
    %vpi_call/w 3 34 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call/w 3 35 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55cf2dd035d0_0 {0 0 0};
    %vpi_call/w 3 36 "$dumpvars", 32'sb00000000000000000000000000000001, v0x55cf2dd3dac0_0 {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 5;
