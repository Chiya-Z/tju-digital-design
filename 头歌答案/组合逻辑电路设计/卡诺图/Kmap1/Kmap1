    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /INPUT 1 "c";
    .port_info 3 /OUTPUT 1 "out";
L_0x559ddb6dd1e0 .functor OR 1, v0x559ddb6dcdc0_0, v0x559ddb6dce80_0, C4<0>, C4<0>;
L_0x559ddb6dd320 .functor OR 1, L_0x559ddb6dd1e0, v0x559ddb6dcf50_0, C4<0>, C4<0>;
