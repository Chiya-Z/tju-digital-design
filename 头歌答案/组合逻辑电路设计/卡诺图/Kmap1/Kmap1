v0x559ddb6dc9e0_0 .net "a", 0 0, v0x559ddb6dcdc0_0;  1 drivers
v0x559ddb6dcaa0_0 .net "b", 0 0, v0x559ddb6dce80_0;  1 drivers
v0x559ddb6dcb70_0 .net "c", 0 0, v0x559ddb6dcf50_0;  1 drivers
v0x559ddb6dcc30_0 .net "out", 0 0, L_0x559ddb6dd320;  alias, 1 drivers
    .scope S_0x559ddb6c7510;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x559ddb6dd050_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x559ddb6dd050_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x559ddb6dd050_0;
    %pad/s 3;
    %split/vec4 1;
    %store/vec4 v0x559ddb6dcf50_0, 0, 1;
    %split/vec4 1;
    %store/vec4 v0x559ddb6dce80_0, 0, 1;
    %store/vec4 v0x559ddb6dcdc0_0, 0, 1;
    %delay 5, 0;
    %load/vec4 v0x559ddb6dd050_0;
    %addi 1, 0, 32;
    %cast2;
    %store/vec4 v0x559ddb6dd050_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %delay 5, 0;
    %vpi_call/w 3 20 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x559ddb6c7510;
T_1 ;
    %vpi_call/w 3 25 "$timeformat", 32'sb11111111111111111111111111110111, 32'sb00000000000000000000000000000000, "ns", 32'sb00000000000000000000000000000101 {0 0 0};
    %vpi_call/w 3 26 "$monitor", "At time %t: a = %b, b = %b, c = %b, out = %b", $time, v0x559ddb6dcdc0_0, v0x559ddb6dce80_0, v0x559ddb6dcf50_0, v0x559ddb6dd0f0_0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x559ddb6c7510;
T_2 ;
